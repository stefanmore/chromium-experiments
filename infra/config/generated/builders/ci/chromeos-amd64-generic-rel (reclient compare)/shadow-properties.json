  "$build/reclient": {
    "ensure_verified": true,
    "instance": "rbe-chromium-untrusted",
    "metrics_project": "chromium-reclient-metrics",
    "rewrapper_env": {
      "RBE_compare": "true",
      "RBE_num_local_reruns": "1",
      "RBE_num_remote_reruns": "1"
    },
    "scandeps_server": true
  }
}